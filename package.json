{
  "name": "nodebb-plugin-postmentions-fixed",
  "version": "1.0.4",
  "description": "NodeBB Plugin that allows users to mention other topics (nodebb-plugin-dbsearch required) and category. Settings in plugins > Post mention.",
  "main": "library.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/boydaihungst/nodebb-plugin-postmentions-fixed"
  },
  "keywords": [
    "nodebb",
    "plugin",
    "postmentions",
    "shell"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "author": {
    "name": "boydaihungst",
    "email": "boydaihungst@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/boydaihungst/nodebb-plugin-postmentions-fixed/issues"
  },
  "readmeFilename": "README.md",
  "nbbpm": {
    "compatibility": "^1.18.6"
  },
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-angular": "15.0.0",
    "cheerio": "^1.0.0-rc.10",
    "eslint": "8.2.0",
    "eslint-config-nodebb": "^0.1.1",
    "eslint-plugin-import": "2.25.3",
    "husky": "7.0.4",
    "lint-staged": "12.0.3",
    "requirejs": "^2.3.6"
  }
}
